<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>EmployeeSearchServlet</servlet-name>
		<display-name>EmployeeSearchServlet</display-name>
		<description></description>
		<servlet-class>com.covalense.empmngmnt.EmployeeSearchServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogOutServlet</servlet-name>
		<display-name>LogOutServlet</display-name>
		<description></description>
		<servlet-class>com.covalense.empmngmnt.LogOutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginPageServlet</servlet-name>
		<display-name>LoginPageServlet</display-name>
		<description></description>
		<servlet-class>com.covalense.empmngmnt.LoginPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeSearchServlet</servlet-name>
		<url-pattern>/EmployeeSearchServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogOutServlet</servlet-name>
		<url-pattern>/LogOutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginPageServlet</servlet-name>
		<url-pattern>/LoginPageServlet</url-pattern>
	</servlet-mapping>

	<!-- <session-config> -->
